 <section style="background: url(http://www.artisstore.com/artisified/assets/img/blur-background08.jpg) no-repeat center center fixed; ">
        <div class="container">
            <div class="row">
     <div class="col-lg-12">
<h2 class="text-center">Sign up</h2>
<hr class="style7">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: 'form-horizontal'}) do |f| %>
<%= devise_error_messages! %>

<div class="form-group">
    <%= f.label :-, class: "col-sm-2 control-label" %>
    
    <div class="col-sm-6 input-group input-group-lg">
     <span class="input-group-addon" id="sizing-addon1">Name</span>

        <%= f.text_field :name, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :-, class: "col-sm-2 control-label" %>
    
    <div class="col-sm-6 input-group input-group-lg">
     <span class="input-group-addon" id="sizing-addon1">Email</span>

        <%= f.email_field :email, class: "form-control" %>
    </div>
</div>

<div class="form-group">
    <%= f.label :-, class: "col-sm-2 control-label" %>

    <div class="col-sm-6 input-group input-group-lg">
     <span class="input-group-addon" id="sizing-addon1">Password</span>

        <%= f.password_field :password, autocomplete: "off", class: "form-control" %>
    </div>
</div>

<div class="form-group">
    <%= f.label :-, class: "col-sm-2 control-label" %>
    
    <div class="col-sm-6 input-group input-group-lg">
     <span class="input-group-addon" id="sizing-addon1">Password Confirm</span>

        <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control" %>
    </div>
</div>

<div class="form-group">
    <div class="col-sm-offset-2 col-sm-4">
        <%= f.submit "Sign up", class: "btn btn-primary" %>
    </div>
      <%= render "users/shared/links" %>
</div>

<% end %>
</div>
</div>
</div>
</section>